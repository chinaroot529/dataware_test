{
    "name": "mysql-doris-cdc-connector",
    "config": {
        "connector.class": "io.debezium.connector.mysql.MySqlConnector",
        "tasks.max": "1",
        "database.hostname": "10.10.0.117",
        "database.port": "6033",
        "database.user": "root",
        "database.password": "Xml123&45!",
        "database.server.id": "184054",
        "database.server.name": "mysql-server",
        "table.include.list": "data_ware_test.abc_warning",
        "database.include.list": "data_ware_test",
        "topic.prefix": "mysql",
        "database.history.kafka.bootstrap.servers": "kafka:29092",
        "database.history.kafka.topic": "mysql.history.data_ware_test",
        "snapshot.mode": "initial",
        "snapshot.locking.mode": "minimal",
        "binlog.buffer.size": "8192",
        "max.batch.size": "2048",
        "max.queue.size": "8192",
        "poll.interval.ms": "1000",
        "key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "key.converter.schemas.enable": "false",
        "value.converter.schemas.enable": "false",
        "transforms": "route,unwrap",
        "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
        "transforms.route.regex": "([^.]+)\\.([^.]+)\\.([^.]+)",
        "transforms.route.replacement": "$3",
        "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
        "transforms.unwrap.drop.tombstones": "false",
        "transforms.unwrap.delete.handling.mode": "rewrite",
        "transforms.unwrap.add.fields": "op,ts_ms,source.db,source.table",
        "database.allowPublicKeyRetrieval": "true",
        "database.useSSL": "false",
        "skipped.operations": "none",
        "tombstones.on.delete": "false",
        "event.processing.failure.handling.mode": "warn",
        "inconsistent.schema.handling.mode": "warn",
        "decimal.handling.mode": "string",
        "time.precision.mode": "connect",
        "bigint.unsigned.handling.mode": "long",
        "include.schema.changes": "false",
        "provide.transaction.metadata": "false",
        "heartbeat.interval.ms": "30000",
        "heartbeat.topics.prefix": "__debezium-heartbeat"
    }
}